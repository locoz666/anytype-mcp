{
  "name": "@anyproto/anytype-mcp",
  "mcpName": "io.github.anyproto/anytype-mcp",
  "keywords": [
    "anytype",
    "api",
    "mcp",
    "server"
  ],
  "version": "1.1.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:dev": "vitest watch",
    "build": "tsc -build && node scripts/build-cli.js",
    "prepare": "npm run build",
    "dev": "tsx watch scripts/start-server.ts",
    "parse-openapi": "tsx scripts/parse-openapi.ts",
    "lint": "eslint \"src/**/*.ts\" \"scripts/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\" \"scripts/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"scripts/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "anytype-mcp": "bin/cli.mjs"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.25.2",
    "axios": "1.13.2",
    "form-data": "4.0.5",
    "mustache": "4.2.0",
    "node-fetch": "3.3.2",
    "openapi-client-axios": "7.8.0",
    "openapi-schema-validator": "12.1.3",
    "openapi-types": "12.1.3",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "0.71.2",
    "@eslint/js": "9.39.2",
    "@types/json-schema": "7.0.15",
    "@types/mustache": "4.2.6",
    "@types/node": "25.0.3",
    "@types/which": "3.0.4",
    "@vitest/coverage-v8": "4.0.16",
    "@typescript-eslint/eslint-plugin": "8.52.0",
    "@typescript-eslint/parser": "8.52.0",
    "eslint": "9.39.2",
    "esbuild": "0.27.2",
    "openai": "6.15.0",
    "prettier": "3.7.4",
    "prettier-plugin-organize-imports": "4.3.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "description": "Official MCP server for Anytype API",
  "repository": {
    "type": "git",
    "url": "https://github.com/anyproto/anytype-mcp.git"
  },
  "author": "@anyproto",
  "bugs": {
    "url": "https://github.com/anyproto/anytype-mcp/issues"
  },
  "homepage": "https://github.com/anyproto/anytype-mcp#readme"
}
